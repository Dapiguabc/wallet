{
  "name": "Lamden_Wallet",
  "version": "0.9.8",
  "description": "A wallet for interacting with contracts on the Lamden blockchain.",
  "scripts": {
    "build": "node utils/build.js",
    "dev": "node utils/webserver.js",
    "unit-test": "./node_modules/.bin/cypress run --config integrationFolder=cypress/unit-tests",
    "test-firstrun-create": "./node_modules/.bin/cypress run --spec cypress/integration/unit-tests/firstrun-create-spec.js",
    "test-firstrun-restore": "./node_modules/.bin/cypress run --spec cypress/integration/unit-tests/firstrun-restore-spec.js  --headed --no-exit",
    "test-lockscreen": "./node_modules/.bin/cypress run --spec cypress/integration/unit-tests/lockscreen-spec.js  --headed --no-exit",
    "test-add-wallet": "./node_modules/.bin/cypress run --spec cypress/integration/unit-tests/add-wallet-spec.js  --headed --no-exit",
    "test-modify-wallet": "./node_modules/.bin/cypress run --spec cypress/integration/unit-tests/modify-wallet-spec.js  --headed --no-exit",
    "test-restore": "./node_modules/.bin/cypress run --spec cypress/integration/unit-tests/restore-spec.js  --headed --no-exit",
    "test-backup": "./node_modules/.bin/cypress run --spec cypress/integration/unit-tests/backup-spec.js  --headed --no-exit"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "2.1.0",
    "cypress": "^3.8.1",
    "cypress-file-upload": "^3.5.0",
    "cypress-localstorage-commands": "^1.1.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "style-loader": "^0.23.1",
    "svelte": "^3.12.1",
    "svelte-loader": "^2.13.4",
    "svg-inline-loader": "^0.8.0",
    "webpack": "4.33.0",
    "webpack-dev-server": "3.7.1",
    "write-file-webpack-plugin": "4.5.0"
  },
  "dependencies": {
    "big-integer": "^1.6.48",
    "capnp-ts": "^0.4.0",
    "cypress-svelte-unit-test": "^1.5.0",
    "jssha": "^2.3.1",
    "md5": "^2.2.1",
    "monaco-editor": "^0.19.0",
    "monaco-editor-webpack-plugin": "^1.8.1",
    "node-cryptojs-aes": "^0.4.0"
  }
}
